{
  "name": "@lzear/eslint-config-typescript",
  "description": "ESLint shareable config for TypeScript",
  "version": "2.1.0",
  "repository": "lzear/eslint-config",
  "author": "lzear",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "config",
    "code-styles",
    "linter",
    "typescript"
  ],
  "scripts": {
    "build": "vite build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
